    <!-- Title Page-->
    <title>Admin</title>

    <!-- Icons font CSS-->
    <link href="../assets/Index/vendor/mdi-font/css/material-design-iconic-font.min.css" rel="stylesheet" media="all">
    <link href="../assets/Index/vendor/font-awesome-4.7/css/font-awesome.min.css" rel="stylesheet" media="all">
    <!-- Font special for pages-->
    <link href="https://fonts.googleapis.com/css?family=Poppins:100,100i,200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">

    <!-- Vendor CSS-->
    <link href="../assets/Index/vendor/select2/select2.min.css" rel="stylesheet" media="all">
    <link href="../assets/Index/vendor/datepicker/daterangepicker.css" rel="stylesheet" media="all">

    <!-- Main CSS-->
    <link href="../assets/Index/css/main.css" rel="stylesheet" media="all">
    <link href="../assets/Index/js/toastr.min.css" rel="stylesheet">
</head>

<body>
    <div class="page-wrapper bg-gra-02 p-t-130 p-b-100 font-poppins">
        <div class="wrapper wrapper--w680">
            <div class="card card-4">
                <div class="card-body">
                    <h2 class="title">Add Products</h2>
                    <form method="POST">
                        <div class="row row-space">
                            <div class="col-2">
                                <div class="input-group">
                                    <label class="label">Product Name</label>
                                    <input class="input--style-4" type="text" name="p_name">
                                </div>
                            </div>
                            <div class="row row-space">
                                <div class="col-2">
                                    <div class="input-group">
                                        <label class="label">Price</label>
                                        <div class="input-group-icon">
                                            <input class="input--style-4" type="text" name="price" placeholder="">
                                        </div>
                                    </div>
                                </div>
                                <!-- <div class="col-2">
                                    <div class="input-group">
                                        <label class="label">Gender</label>
                                        <div class="p-t-10">
                                            <label class="radio-container m-r-45">Male
                                            <input type="radio" checked="checked" name="gender">
                                            <span class="checkmark"></span>
                                        </label>
                                            <label class="radio-container">Female
                                            <input type="radio" name="gender">
                                            <span class="checkmark"></span>
                                        </label>
                                        </div>
                                    </div>
                                </div>
                            </div> -->
                                <div class="row row-space">
                                    <div class="col-2">
                                        <div class="input-group">
                                            <label class="label">Size</label>
                                            <input id="pass" class="input--style-4" type="text" name="size">
                                        </div>
                                    </div>
                                    <div class="col-2">
                                        <div class="input-group">
                                            <label class="label">Product Description</label>
                                            <input class="input--style-4" type="text" name="p_description">
                                        </div>
                                    </div>
                                </div>
                                <div class="input-group">
                                    <label class="label">Category</label>
                                    <div class="rs-select2 js-select-simple select--no-search">
                                        <select name="category">
                                    <option value="" selected="selected">Choose option</option>
                                    <% for(const cat of category ){ %>
                                        <option value="<%= cat.c_Id %>"><%= cat.c_name %></option>
                                        <% } %>
                                    
                                </select>
                                        <div class="select-dropdown"></div>
                                    </div>
                                </div>
                                <div class="p-t-15">
                                    <button id="button" class="btn btn--radius-2 btn--blue" type="submit">Submit</button>
                                </div>
                    </form>
                    </div>
                    </div>
                </div>
            </div>

            <!-- Jquery JS-->
            <script src="../assets/Index/vendor/jquery/jquery.min.js"></script>
            <!-- Vendor JS-->
            <script src="../assets/Index/vendor/select2/select2.min.js"></script>
            <script src="../assets/Index/vendor/datepicker/moment.min.js"></script>
            <script src="../assets/Index/vendor/datepicker/daterangepicker.js"></script>

            <!-- Main JS-->
            <script src="../assets/Index/js/global.js"></script>
            <script src="../assets/Index/js/toastr.min.js"></script>

</body>
